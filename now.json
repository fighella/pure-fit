{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@now/static" },
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "name": "pure-node",
  "alias": [
    "www.pureyogaottawa.com"
  ],
  "routes": [
    { "src": "/team/(?<id>[^/]*)", "dest": "/team_show?handle=$id" },
    { "src": "/team/(?<id>[^/]*)/embed", "dest": "/team_frame?handle=$id" },
    { "src": "/journals/(?<id>[^/]*)", "dest": "/journals_show?handle=$id" },
    { "src": "/evolve/(?<id>[^/]*)/(?<handle>[^/]*)", "dest": "/evolve_show?id=$id&handle=$handle" },
    { "src": "/schedule/(?<id>[^/]*)", "dest": "/schedule?handle=$id" },
    { "src": "/location/(?<id>[^/]*)", "dest": "/location?location=$id" },
    { "src": "/class_styles/(?<id>[^/]*)", "dest": "/class_styles_show?handle=$id" },
    { "src": "/online/(?<id>[^/]*)", "dest": "/online_show?handle=$id" },
    { "src": "/intro", "status": 301, "headers": { "Location": "https://www.pureyogaottawa.com/new" } },
    { "src": "/packages", "status": 301, "headers": { "Location": "https://www.pureyogaottawa.com/pricing" } },
    { "src": "/workshops", "status": 301, "headers": { "Location": "https://www.pureyogaottawa.com/evolve" } },
    { "src": "/hot-yoga-schedule", "status": 301, "headers": { "Location": "https://www.pureyogaottawa.com/schedule" } },
    { "src": "/pure-team", "status": 301, "headers": { "Location": "https://www.pureyogaottawa.com/team" } },
    { "src": "/pure-team/(?<id>[^/]*)", "status": 301, "headers": { "Location": "https://www.pureyogaottawa.com/team/$id" } },
    { "src": "/p/contact-us", "status": 301, "headers": { "Location": "https://www.pureyogaottawa.com/contact" } },
    { "src": "/yoga-class-styles/(?<id>[^/]*)", "status": 301, "headers": { "Location": "https://www.pureyogaottawa.com/class_styles/$id" } }
  ]
}